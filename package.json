  "build": {
    "appId": "com.pictallion.desktop",
    "productName": "Pictallion",
    "icon": "assets/icon.ico",
    "files": [
      "electron/**/*",
      "client/**/*",
      "server/**/*",
      "shared/**/*",
      "data/**/*",
      "dist/**/*",
      "package.json"
    ],
    "directories": {
      "buildResources": "build"
    },
    "mac": {
      "target": [
        "dmg"
      ],
      "icon": "assets/icon.icns"
    },
    "win": {
      "target": [
        "nsis",
        "msi"
      ],
      "icon": "assets/icon.ico"
    },
    "linux": {
      "target": [
        "AppImage",
        "deb",
        "rpm"
      ],
      "category": "Photography",
      "icon": "assets/icon.png"
    }
  }
